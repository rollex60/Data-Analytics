{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi":false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^/components/(.*)$",
		"^/hooks/(.*)$",
    "^/ui/(.*)$",
    "^/screens/(.*)$",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}